{
  "name": "image-converter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/converter/test/jest-e2e.json",
    "docker:dev": "docker-compose -f docker-compose.dev.yaml up -d",
    "docker:prod": "docker-compose -f docker-compose.yaml up -d"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/microservices": "^9.1.4",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^3.0.0",
    "amqp-connection-manager": "^4.1.7",
    "amqplib": "^0.10.3",
    "app-root-path": "^3.0.0",
    "date-fns": "^2.21.3",
    "fs-extra": "^9.1.0",
    "nestjs-rmq": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sharp": "^0.28.3",
    "typescript": "^4.3.2",
    "update": "^0.7.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@nrwl/cli": "^14.3.6",
    "@nrwl/eslint-plugin-nx": "^14.3.6",
    "@nrwl/jest": "^14.3.6",
    "@nrwl/linter": "^14.3.6",
    "@nrwl/nest": "^14.3.6",
    "@nrwl/node": "^14.3.6",
    "@nrwl/nx-cloud": "^14.7.0",
    "@nrwl/workspace": "14.3.6",
    "@types/amqplib": "^0.8.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.5",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/sharp": "^0.28.2",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4",
    "webpack": "^5.38.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
